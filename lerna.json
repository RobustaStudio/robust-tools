{
	"packages": ["packages/*"],
	"version": "independent",
	"publishConfig": {
		"access": "public"
	},
	"command": {
		"publish": {
			"conventionalCommits": true,
			"message": "chore(release): publish",
			"access": "public",
			"npmClient": "npm",
			"allowBranch": ["master", "feature/*"]
		}
	}
}
